{
  "compilerOptions": {
    "lib": ["es6", "esnext.asynciterable"],
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitReturns": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "rootDir": "./",
    "outDir": ".build"
  }
}
